export BASH_SILENCE_DEPRECATION_WARNING=1 # macOS
export EDITOR=vim

source ~/.git_aliases

EDITOR='vim'

alias v='vim'
alias ls='ls -lartF'
mcdir () {
    mkdir -p "$1"
    cd "$1"
}
alias gaml='yq eval --output-format=props'
alias p='pnpm'
alias pi='pnpm i'
alias tf='terraform'
alias python=python3
